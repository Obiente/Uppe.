---
interface Props {
  startLabel?: string;
  endLabel?: string;
  class?: string;
}

const { startLabel = '24 hours ago', endLabel = 'Now', class: className = '' } = Astro.props;
---

<div class={`flex justify-between text-xs text-tertiary ${className}`}>
  <span>{startLabel}</span>
  <span>{endLabel}</span>
</div>
