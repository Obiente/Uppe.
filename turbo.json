{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "target/release/**",
        "bin/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "env": ["LocalAppData", "GOCACHE", "GOPATH"]
    },
    "@uppe/server#dev": {
      "persistent": true,
      "cache": false,
      "interruptible": true,
      "env": ["LocalAppData", "GOCACHE", "GOPATH"]
    },
    "proto": {
      "dependsOn": [
        "^proto"
      ],
      "outputs": [
        "gen/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "deps": {
      "cache": false
    }
  }
}
